{
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "javascript.format.enable": false,
  "typescript.format.enable": false,
  "prettier.requireConfig": true,
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "files.associations": {
    "*.js": "javascript"
  },
  "cursor.formatOnSave": false,
  "cursor.autoFormat": false,
  "cursor.formatOnPaste": false,
  "cursor.formatOnType": false,
  "cursor.formatOnSaveMode": "manual",
  "cursor.formatOnFocusChange": false,
  "cursor.formatOnWindowChange": false,
  "cursor.formatOnFileChange": false,
  "cursor.formatOnWorkspaceChange": false,
  "cursor.formatOnExtensionChange": false,
  "cursor.formatOnLanguageChange": false,
  "cursor.formatOnThemeChange": false,
  "cursor.formatOnSettingsChange": false,
  "cursor.formatOnConfigurationChange": false,
  "cursor.formatOnStartup": false,
  "cursor.formatOnShutdown": false,
  "cursor.formatOnReload": false,
  "cursor.formatOnRestart": false
}
